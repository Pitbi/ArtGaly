<div class="page-header">
  <h2>
		Livres/nouvelles
    <a class="pull-right btn btn-small action show-form-btn">
      Ajouter un livre
    </a>
	</h2>
</div>

<div class="row-fluid">
  <%- partial("books/_newBook", {book: book}) %>
</div>

<% books.forEach(function (book) { %>
	<a href="/book/<%= book.id %>">
		<div class="book-cover">
			<div class="book-cover-title">
					<span><%= book.name %></span>
			</div>
		</div>
	</a>
<% }); %>



