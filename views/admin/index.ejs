<div class="page-header">
  <h3>Administration</h3>
</div>

<div class="row-fluid">
  <h4>Images en page d'acceuil</h4>

  <% pictures.forEach(function (picture) { %>
    <% if (picture.homePage) { %>
      <div class="album-box">
        <a href="/picture/<%= picture.id %>">
          
            <img src="<%= picture.path %>">
          
          <%= picture.name %>
        </a>
      </div>
    <% } %>
  <% }); %>
</div>

<div class="row-fluid">
  <h4>Images dans album</h4>

  <% pictures.forEach(function (picture) { %>
    <% console.log(picture); %>
    <% if (!picture.album) { %>
      <div class="album-box">
        <a href="/picture/<%= picture.id %>">
          
          <img src="<%= picture.path %>">
          
          <%= picture.name %>
        </a>
      </div>
    <% } %>
  <% }); %>
</div>
